{
	"extends": ["tslint-plugin-prettier", "tslint-config-prettier"],
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [ "always" ],
		"triple-equals": true,
		"no-var-keyword": true,
		"no-bitwise": true,
		"prettier": true
	}
}